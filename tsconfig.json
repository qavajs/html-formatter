{
    "compilerOptions": {
        "esModuleInterop": true,
        "isolatedModules": true,
        "jsx": "react-jsx",
        "module": "node16",
        "moduleResolution": "node16",
        "noEmit": true,
        "resolveJsonModule": true,
        "strict": true,
        "strictNullChecks": false,
        "skipLibCheck": true,
        "target": "ES2017",
        "rootDir": ".",
        "types": [
            "vitest/globals",
            "vitest-dom/extend-expect"
        ]
    }
}
